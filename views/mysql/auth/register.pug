doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no")
    meta(name="description" content="")
    meta(name="author" content="")
    title 탭타이틀
    style
      include ../../../css/util.css
      include ../../../css/main.css
      include ../../../vendor/bootstrap/css/bootstrap.min.css
      include ../../../css/clean-blog.min.css
  body
    // Navigation
    nav#mainNav.navbar.navbar-expand-lg.navbar-light.fixed-top
      .container
        a.navbar-brand(href="index.html") 왼쪽위 타이틀
        button.navbar-toggler.navbar-toggler-right(type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation")
          | Menu
          i.fas.fa-bars
        #navbarResponsive.collapse.navbar-collapse
          ul.navbar-nav.ml-auto
            li.nav-item
              a.nav-link(href="/") 오른쪽위(로그인따라다름)
            li.nav-item
              a.nav-link(href="/") Home
            if user
              li.nav-item
                a.nav-link(href="/auth/logout") logout
            else
              li.nav-item
                a.nav-link(href="/auth/login") login
              li.nav-item
                a.nav-link(href="/auth/register") register
    // Page Header
    header.masthead(style="background-image: url('data:image/jpeg;base64,')")
      .overlay
      .container
        .row
          .col-lg-8.col-md-10.mx-auto
            .site-heading
              h1 정중앙 큰글
              span.subheading 정중앙 큰글아래 작은글
    // Main Content
    .container
      .row
        .col-lg-8.col-md-10.mx-auto
          .post-preview
            span.login100-form-title.p-b-32
              | Register
            form.login100-form(action='/auth/register', method='post')
              span.txt1.p-b-11
                | Username
              div
                input.input100(type='text', name='username', placeholder='username')
              span.txt1.p-b-11
                | Password
              div
                input.input100(type='password', name='password', placeholder='password')
              span.txt1.p-b-11
                | Password Check
              div
                input.input100(type='text', name='displayName', placeholder='password ckeck')
              div
                input.login100-form-btn(type='submit',style="margin-top:18px;")
          // Pager
          .clearfix
    hr
    // Footer
    footer
      .container
        .row
          .col-lg-8.col-md-10.mx-auto
            ul.list-inline.text-center
              li.list-inline-item
                a(href="#")
                  span.fa-stack.fa-lg
                    i.fas.fa-circle.fa-stack-2x
                    i.fab.fa-twitter.fa-stack-1x.fa-inverse
              li.list-inline-item
                a(href="#")
                  span.fa-stack.fa-lg
                    i.fas.fa-circle.fa-stack-2x
                    i.fab.fa-facebook-f.fa-stack-1x.fa-inverse
              li.list-inline-item
                a(href="#")
                  span.fa-stack.fa-lg
                    i.fas.fa-circle.fa-stack-2x
                    i.fab.fa-github.fa-stack-1x.fa-inverse
            p.copyright.text-muted Copyright © Your Website 2018
    // Bootstrap core JavaScript
    script(src="./vendor/jquery/jquery.min.js")
    script(src="./vendor/bootstrap/js/bootstrap.bundle.min.js")
    // Custom scripts for this template
    script(src="./js/clean-blog.min.js")
